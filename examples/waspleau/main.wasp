app waspleau {
  title: "Waspleau"
}

server {
  setupFn: import serverSetup from "@ext/serverSetup.js"
}

db {
  system: PostgreSQL
}

route "/" -> page Main
page Main {
  component: import Main from "@ext/MainPage.js"
}

query dashboard {
