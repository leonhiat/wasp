
datasource db {
  provider = "sqlite"
  url = "***"
}

generator client {
  provider = "prisma-client-js"
  output = "../server/node_modules/.prisma/client"
}

model User {
    id          Int     @id @default(autoincrement())
    email       String  @unique
    password    String
}

model Task {
    id          Int     @id @default(autoincrement())
    description String
    isDone      Boolean @default(false)
}

