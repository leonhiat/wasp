app waspJob {
  db: { system: PostgreSQL },
  wasp: {
    version: "^0.6.0"
  },
  title: "waspJob"
}

route RootRoute { path: "/", to: MainPage }
page MainPage {
  component: import Main from "@ext/MainPage.js"
}
 job MySpecialJob { 
   executor: PgBoss, 
   perform: { 
     fn: import { foo } from "@ext/jobs/bar.js"  
   } 
 } 

