// Test .wasp file.

// App definition.
app test_app {
    // Title of the app.
    title: "Hello World!"
}

// Page definition.
page Landing {
    route: "/",
    content: {=jsx
        <div>My landing page! I have { this.props.taskList.length } tasks. </div>
    jsx=}
}

page TestPage {
    route: "/test",
    content: {=jsx
        <div>This is a test page!</div>
    jsx=}
}

// Entity definition.
entity Task {
    description :: string,
    isDone :: boolean
}
